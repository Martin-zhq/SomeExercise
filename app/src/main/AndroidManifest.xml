<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.xxt.recyclerviewexercise">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".transmitdata.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".recyclerviewexercise.ShowDateTime"
            android:label="@string/date_and_time" />
        <activity android:name=".transmitdata.TransmitDataActivity" />
        <activity android:name=".transmitdata.MyActivity1" />
        <activity android:name=".transmitdata.MyActivity2" />
        <activity android:name=".transmitdata.MyActivity3" />
        <activity android:name=".transmitdata.MyActivity4" />
        <activity android:name=".startactivityforresult.ResultActivity" />
        <activity android:name=".startactivityforresult.RequestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>